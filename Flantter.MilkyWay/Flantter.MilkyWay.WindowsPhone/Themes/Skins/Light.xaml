<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Flantter.MilkyWay.Themes">

    <SolidColorBrush x:Key="WindowsPhone_PageBackgroundBrush" Color="#FFEBEBEB" />

    <SolidColorBrush x:Key="WindowsPhone_BottomBarBackgroundBrush" Color="#FF2387C1" />
    <SolidColorBrush x:Key="WindowsPhone_BottomBarTextblockButtonSelectedBrush" Color="White" />
    <SolidColorBrush x:Key="WindowsPhone_BottomBarTextblockButtonUnselectedBrush" Color="Gray" />
    <SolidColorBrush x:Key="WindowsPhone_BottomBarButtonSelectedBackgroundBrush" Color="#FFEBEBEB" />
    <SolidColorBrush x:Key="WindowsPhone_BottomBarButtonSelectedForegroundBrush" Color="Black" />
    <SolidColorBrush x:Key="WindowsPhone_BottomBarButtonUnselectedBackgroundBrush" Color="#FF2387C1" />
    <SolidColorBrush x:Key="WindowsPhone_BottomBarButtonUnselectedForegroundBrush" Color="White" />
    <SolidColorBrush x:Key="WindowsPhone_BottomBarAppBarButtonItemBackgroundThemeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="WindowsPhone_BottomBarAppBarButtonItemDisabledForegroundThemeBrush" Color="#66FFFFFF" />
    <SolidColorBrush x:Key="WindowsPhone_BottomBarAppBarButtonItemForegroundThemeBrush" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="WindowsPhone_BottomBarAppBarButtonItemPressedForegroundThemeBrush" Color="#FF000000" />

</ResourceDictionary>
